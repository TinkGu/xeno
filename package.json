{
  "name": "@tinks/xeno",
  "version": "1.0.0",
  "description": "工具库及其在线文档",
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/index.d.ts",
  "npmName": "@tinks/xeno",
  "npmFileMap": [
    {
      "basePath": "/lib/",
      "files": [
        "*.js"
      ]
    }
  ],
  "license": "ISC",
  "files": [
    "lib",
    "fetch",
    "react",
    "navigate"
  ],
  "scripts": {
    "start": "yarn build && yarn docs",
    "lint": "eslint src --quiet --ext ts,tsx,js,jsx",
    "lint:ts": "tsc --noEmit",
    "docs": "yarn docs:inject && yarn docs:ts -c && cross-env APP_ROOT=docs dumi dev",
    "docs:build": "node -v && yarn docs:inject && yarn docs:ts-prod && cross-env APP_ROOT=docs dumi build",
    "docs:ts": "node docs/ts-doc-parser -d",
    "docs:ts-prod": "node docs/ts-doc-parser -c",
    "docs:inject": "node docs/inject",
    "test": "jest",
    "build": "rm -rf lib && yarn build:esm && yarn build:cjs && yarn build:filemap",
    "build:filemap": "node scripts/build-file-map.js",
    "build:esm": "tsc --module es2015 --target es5 --outDir lib/esm --project tsconfig.build.json",
    "build:cjs": "tsc --module commonjs --target es5 --outDir lib --project tsconfig.build.json",
    "prerelease": "yarn lint && yarn lint:ts && yarn build",
    "release": "standard-version --preset angular",
    "release:beta": "yarn prerelease && standard-version --prerelease beta",
    "postpublish": "git push origin HEAD --follow-tags && git fetch --all"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --ext js,ts,tsx --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint:ts",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "@types/qs": "6.9.1",
    "lodash": "^4.17.21",
    "qs": "^6.9.1"
  },
  "peerDependencies": {
    "axios": "^0.26.0",
    "history": "^4.10.1",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "@types/history": "^4.7.5",
    "@types/jest": "^26.0.23",
    "@types/node": "13.7.7",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "axios": "^0.26.0",
    "conventional-changelog-angular": "^5.0.13",
    "create-npm-file-map": "1.3.1",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "dumi": "1.1.40",
    "eslint": "7.13.0",
    "eslint-config-react-ts-sonar": "^1.8.1",
    "fs-extra": "^10.1.0",
    "husky": "4.3.0",
    "jest": "^27.0.4",
    "lint-staged": "10.5.4",
    "prettier": "^2.5.1",
    "qrcode.react": "^3.0.1",
    "react": "^17.0.1",
    "standard-version": "9.1.1",
    "ts-jest": "^27.0.3",
    "typedoc": "0.22.15",
    "typescript": "^4.5.15"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "resolutions": {
    "typedoc": "0.22.9"
  },
  "engines": {
    "node": "^10.12.0 || >=12.0.0"
  }
}
